          "_customHTML": "<section class=\"content12\" group=\"Content\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" name=\"textWidth\" title=\"Width\" min=\"4\" max=\"12\" value=\"10\" step=\"1\">\n        <header>Background</header>\n        <input type=\"color\" title=\"Color\" name=\"bgColor\" value=\"#ffffff\">\n    </mbr-parameters>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-md-12 col-lg-{{textWidth}}\">\n                <div class=\"mbr-section-btn align-center\" mbr-theme-style=\"display-7\" data-toolbar=\"-mbrBtnMove,-mbrAddBtn\">\n                    <a id=\"btnPrev\" class=\"btn btn-primary\" href=\"step2.html\">&larr; Step 2: TV Setup</a>\n                    <a id=\"btnNext\" class=\"btn btn-primary\" href=\"task.html\">Schedule Updates &rarr;</a>\n                </div>\n            </div>\n        </div>\n    </div>\n    <script>\n        (function (w) {\n            w.URLSearchParams = w.URLSearchParams || function (searchString) {\n                var self = this;\n                self.searchString = searchString;\n                self.get = function (name) {\n                    var results = new RegExp('[\\?&]' + name + '=([^&#]*)').exec(self.searchString);\n                    if (results == null) {\n                        return null;\n                    }\n                    else {\n                        return decodeURI(results[1]) || 0;\n                    }\n                };\n            }\n        })(window)\n        \n        window.onload = function updateButtons()\n        {\n            var nextBtn = document.getElementById(\"btnNext\");\n            nextBtn.href += window.location.search + \"#full\";\n\n            var prevBtn = document.getElementById(\"btnPrev\");\n            prevBtn.href += window.location.search;\n\n            var urlParams = new URLSearchParams(window.location.search);\n            if (urlParams.get(\"client\") == \"none\")\n            {\n                prevBtn.href = \"install.html\";\n                prevBtn.innerHTML = \"&larr; Install\";\n                nextBtn.href = \"task.html\" + window.location.search + \"#server\";\n            }\n        }\n    </script>\n</section>",
